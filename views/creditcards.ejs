  color: white;
  border: none;
  border-radius: 8px;
  padding: 10px 15px;
  font-weight: bold;
  cursor: pointer;
  flex: 1;
  font-size: 12px;
  transition: background-color 0.2s ease;
}

.buttons button:hover {
  background-color: #e67300;
}
